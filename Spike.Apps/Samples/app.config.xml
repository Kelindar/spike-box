<?xml version="1.0"?>
<Configuration xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Hosts>
    <Host Name="*misakai.com" />
    <Host Name="*.misakai.com" />
  </Hosts>
  <Rules>
    <Rule Source="/timer$" Destination="/app/timer/index" />
    <Rule Source="/todo$" Destination="/app/todo/index" />
    <Rule Source="/hello$" Destination="/app/hello/index" />
    <Rule Source="/form$" Destination="/app/form/index" />
    <Rule Source="/list$" Destination="/app/list/index" />
    <Rule Source="/table$" Destination="/app/table/index" />
    <Rule Source="/setter$" Destination="/app/setter/index" />
    <Rule Source="/nested$" Destination="/app/nested/index" />
    <Rule Source="/compose$" Destination="/app/compose/index" />
	<Rule Source="/controls$" Destination="/app/controls/index" />
    <Rule Source="/buffer$" Destination="/app/buffer/index" />
	<Rule Source="/login$" Destination="/app/login/index" />
	<Rule Source="/file$" Destination="/app/file/index" />
  </Rules>
  <Elements>
	<Element Name="notify" Template="controls/notify" Transclude="true" />
	<Element Name="icon" Template="controls/icon" Transclude="false" />
	<Element Name="well" Template="controls/well" Transclude="true" />
	<Element Name="well-lg" Template="controls/well-lg" Transclude="true" />
	<Element Name="well-sm" Template="controls/well-sm" Transclude="true" />
  </Elements>
</Configuration>